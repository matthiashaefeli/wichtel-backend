<h1>Nachrichten</h1>
<p><%= link_to 'Home', root_path %></p>
<% @questions.each do |q| %>
  <div class='questionsHome'>
    <p>Name:<%= q.name %></p>
    <p>Email:<%= q.email %></p>
    <p>Betreff:<%= q.subject %></p>
    <p>Nachricht:<%= q.message %></p>
    <%= link_to 'Nachricht Loeschen', question_path(q.id), method: :delete %>
  </div>
<% end %>